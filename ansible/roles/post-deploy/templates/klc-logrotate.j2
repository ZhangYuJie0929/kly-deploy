/var/log/emqx/*.log
/var/log/libvirt/*.log
/var/log/mariadb/*.log
/var/log/trochilus/*.log
{% if enable_guest_service | bool %}
/var/log/guest-service/*.log 
{% endif %}
{
    daily
    rotate 30
    compress
    missingok
    notifempty
    delaycompress
    create 644 root root
}

/var/log/redis/*.log
{
    daily
    rotate 30
    compress
    missingok
    notifempty
    delaycompress
    create 666 root root
}
