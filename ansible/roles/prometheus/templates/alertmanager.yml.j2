global:
  resolve_timeout: "1m"
route:
  group_by:
    - "alertname"
  group_wait: "10s"
  group_interval: "1m"
  repeat_interval: "100y"
  receiver: "web.hook"
receivers:
  - name: "web.hook"
    webhook_configs:
    - url: "http://:{{ fsd_webhook_port }}/api/v1/alarm/log/webhook"
inhibit_rules:
  - source_match:
      severity: "critical"
    target_match:
      severity: "warning"
    equal:
      - "alertname"
